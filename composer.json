{
  "name": "kelvinsouza/translate",
  "description": "Tradução automática para Laravel.",
  "homepage": "https://github.com/KelvinDeveloper/translate",
  "keywords": [
    "translate",
    "laravel translate",
    "google translate",
    "aws translate",
    "auto translate"
  ],
  "authors": [
    {
      "name": "Kelvin Souza",
      "email": "kelvin.developer@icloud.com"
    }
  ],
  "license": "MIT",
  "type": "lib",
  "version": "0.1.2",
  "require": {
    "aurawindsurfing/google-translate": "*",
    "predis/predis": "^1.1",
    "aws/aws-sdk-php-laravel": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Translate\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Translate\\Providers\\TranslateProvider"
      ],
      "aliases": {
        "Translate": "Translate\\Facades\\TranslateFacade"
      }
    }
  },
  "minimum-stability": "stable"
}